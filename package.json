{
  "name": "lazy-loading-without-router",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "libs": "cat angular.json | jq '.projects | to_entries[] | select(.value.projectType == \"library\").key'",
    "libs:watch:paths": "yarn --silent libs | xargs -n1 -I PROJECT bash -c \"echo  '--watch ./projects/'PROJECT\"",
    "build:libs": "yarn --silent libs | xargs -n1 -I PROJECT bash -c 'yarn build PROJECT'",
    "start:fresh": "rm -rf dist && yarn build:libs && yarn start"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.8",
    "@angular/common": "^7.2.8",
    "@angular/compiler": "^7.2.8",
    "@angular/core": "^7.2.8",
    "@angular/forms": "^7.2.8",
    "@angular/http": "^7.2.8",
    "@angular/platform-browser": "^7.2.8",
    "@angular/platform-browser-dynamic": "^7.2.8",
    "@angular/router": "^7.2.8",
    "core-js": "^2.5.4",
    "ramda": "^0.26.1",
    "rxjs": "~6.4.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.5",
    "@angular/compiler-cli": "^7.2.8",
    "@angular/language-service": "^7.2.8",
    "@types/jasmine": "~3.3.9",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~11.11.0",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.1.0",
    "protractor": "~5.4.0",
    "ts-node": "~8.0.3",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.13.1",
    "typescript": "<3.3.0"
  }
}
